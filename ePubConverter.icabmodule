//startconfig
//id=de.icab.ePubConverter

//title=Convert to ePub
//description=Converts the current page to ePub e-book format.
//var=confirmation;type=confirmation;default=false;
//var=out_lang;title=Output Language;default=en
//endconfig

    var doAction = (confirmation == false);

    if (confirmation) {
        var text = "Do you want to convert this page to ePub format?";
        doAction = confirm(text);
    }

    if (doAction) {
      var d = document;
      try {
        if(!d.body || d.body.innerHTML == '') {
          throw(0);
        }

        var dotEPUBcss = d.createElement('link');
        dotEPUBcss.rel = 'stylesheet';
        dotEPUBcss.href = 'http://dotepub.com/s/dotEPUB-favlet.css';
        dotEPUBcss.type = 'text/css';dotEPUBcss.media='screen';
        d.getElementsByTagName('head')[0].appendChild(dotEPUBcss);

        dotEPUBstatus = d.createElement('div');
        dotEPUBstatus.setAttribute('id','dotepub');
        dotEPUBstatus.innerHTML = '<div id="status"><p>working...</p></div>';
        d.body.appendChild(dotEPUBstatus);
 
        var dotEPUB = d.createElement('script');
        dotEPUB.type = 'text/javascript';
        dotEPUB.charset = out_lang;
        dotEPUB.src = 'http://dotepub.com/j/dotepub.js?x='+(Math.random());
        dotEPUB_links = 1;
        dotEPUB_lang = out_lang;
        d.getElementsByTagName('head')[0].appendChild(dotEPUB);
      } catch(e) {
        alert('The page has no content or it is not fully loaded. Please, wait till the page is loaded.');
      }
    }
